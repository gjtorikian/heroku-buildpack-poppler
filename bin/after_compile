export PATH="/app/vendor/fontconfig/bin:/app/vendor/libpoppler/bin:$PATH"
export LD_RUN_PATH="/app/vendor/fontconfig/lib:/app/vendor/libpoppler/lib:$LD_RUN_PATH"
export LD_LIBRARY_PATH="/app/vendor/fontconfig/lib:/app/vendor/libpoppler/lib:$LD_LIBRARY_PATH"
export PKG_CONFIG_PATH="/app/vendor/fontconfig/lib/pkgconfig:/app/vendor/libpoppler/lib/pkgconfig:$PKG_CONFIG_PATH"

echo "-----> /app/vendor contents:"
ls -a /app/vendor

echo "-----> /app/vendor/fontconfig/lib contents:"
ls -a /app/vendor/fontconfig/lib
